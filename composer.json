{
    "name": "netglue/sitemap-builder",
    "description": "A Simple XML Sitemap generation utility",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "George Steel",
            "email": "george@net-glue.co.uk"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php" : ">=7.1",
        "ext-xmlwriter" : "*",
        "zendframework/zend-uri": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Netglue\\Sitemap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netglue\\SitemapTest\\": "test/SitemapTest/"
        }
    }
}
